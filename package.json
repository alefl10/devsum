{
  "name": "devsum",
  "version": "1.0.0",
  "description": "Social media Web App for software developers",
  "main": "index.js",
  "scripts": {
    "client-install": "npm start --prefix client",
    "server": "babel-node index.js",
    "dev-server": "nodemon --exec babel-node index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alefl10/devsum.git"
  },
  "keywords": [
    "webapp",
    "mern",
    "node",
    "express",
    "react"
  ],
  "author": "Alejandro Ferrero",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alefl10/devsum/issues"
  },
  "homepage": "https://github.com/alefl10/devsum#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "concurrently": "^4.1.0",
    "express": "^4.18.2",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.14",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.10"
  }
}
